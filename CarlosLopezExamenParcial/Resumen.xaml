<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CarlosLopezExamenParcial.Resumen">
    <ContentPage.Content>
        <StackLayout>
            <Frame BackgroundColor="White" CornerRadius="0">
                <Image Source="logo.png" Aspect="AspectFit" HeightRequest="100" HorizontalOptions="Center"/>
            </Frame>
            <Frame  BackgroundColor="#2196F3" Padding="0" CornerRadius="0">
                <Label Text="Resumen de Inscripción" HorizontalTextAlignment="Center" TextColor="White" FontSize="12"/>
            </Frame>
            <Entry x:Name="txtnombreusuario"  IsEnabled="False"/>
            <Entry x:Name="txtusuarioconectado" IsEnabled="False" />
            <Entry x:Name="txtPagoFinal" IsEnabled="False" />
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>